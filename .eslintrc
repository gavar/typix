{
  "extends": [
    "@typix/eslint-config",
    "@typix/eslint-config/lodash",
    "@typix/eslint-config/typescript",
    "@typix/eslint-config/typescript/jsdoc",
    "@typix/eslint-config/typescript/allow-js"
  ],
  "rules": {
    "jsdoc/require-jsdoc": "off",
    "jsdoc/require-param": "off",
    "jsdoc/require-returns": "off"
  }
}
